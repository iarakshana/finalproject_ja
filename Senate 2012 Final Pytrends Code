    "from pytrends.request import TrendReq\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "# Login to Google. Only need to run this once, the rest of requests will use the same session.\n",
    "pytrend = TrendReq()\n",
    "\n",
    "#AZ\n",
    "pytrend.build_payload(kw_list=[\"Jeff Flake\", \"Richard Carmona\"], geo = 'US',  timeframe = '2012-01-01 2012-11-06')\n",
    "iot_2012_az = pytrend.interest_over_time()\n",
    "JFlakep = (sum(iot_2012_az[\"Jeff Flake\"]))/ ((sum(iot_2012_az[\"Jeff Flake\"])) + (sum(iot_2012_az[\"Richard Carmona\"])))\n",
    "RCarmp = 1-JFlakep\n",
    "gtrends2012S = pd.Series([JFlakep, RCarmp])\n",
    "\n",
    "#CA\n",
    "pytrend.build_payload(kw_list=['Dianne Feinstein', 'Elizabeth Emken'], geo = 'US',  timeframe = '2012-01-01 2012-11-06')\n",
